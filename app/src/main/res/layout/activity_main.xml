<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/greywall"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/logoutbtn"
        android:layout_width="146dp"
        android:layout_height="58dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="212dp"
        android:layout_marginEnd="105dp"
        android:layout_marginBottom="81dp"
        android:background="@drawable/wall"
        android:text="Logout"
        android:textAlignment="center"
        android:textStyle="bold"
        android:onClick="logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Trashlevelstatusbtn" />

    <Button
        android:id="@+id/returnhomebtn"
        android:layout_width="202dp"
        android:layout_height="53dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="105dp"
        android:layout_marginBottom="37dp"
        android:background="@drawable/wall"
        android:text="Return Home"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/logoutbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Trashlevelstatusbtn" />

    <Button
        android:id="@+id/Notificationbtn"
        android:layout_width="202dp"
        android:layout_height="53dp"
        android:layout_marginStart="104dp"
        android:layout_marginEnd="105dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/wall"
        android:text="Notifications"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/returnhomebtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Trashlevelstatusbtn" />

    <Button
        android:id="@+id/Trashlevelstatusbtn"
        android:layout_width="202dp"
        android:layout_height="53dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="105dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/wall"
        android:text="Trash Level Status"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Notificationbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/drivetocurbbtn" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="232dp"
        android:layout_height="75dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="122dp"
        android:layout_marginEnd="138dp"
        android:layout_marginBottom="83dp"
        android:fontFamily="@font/berkshire_swash"
        android:text="Welcome"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="?android:attr/windowTitleSize"
        app:layout_constraintBottom_toTopOf="@+id/Trashlevelstatusbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.392"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.209" />

    <Button
        android:id="@+id/drivetocurbbtn"
        android:layout_width="202dp"
        android:layout_height="53dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="105dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/wall"
        android:text="Drive to Curb"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Trashlevelstatusbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="212dp"
        android:layout_height="127dp"
        android:layout_marginStart="94dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="105dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/robottrashcan" />

</androidx.constraintlayout.widget.ConstraintLayout>